{
  "name": "platmatic",
  "productName": "platmatic",
  "version": "0.1.0",
  "description": "Desktop application for interacting with cloud foundry",
  "main": "main.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test mocha --compilers ts:ts-node/register --recursive --require should --require ./test-setup.ts app/**/*.spec.ts*",
    "test:watch": "npm test -- --watch --watch-extensions tsx,ts",
    "lint": "eslint app test *.js",
    "hot-server": "cross-env NODE_ENV=development node -r babel-register server.js",
    "build-main": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.electron.js --progress --profile --colors",
    "build-renderer": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.production.js --progress --profile --colors",
    "build": "npm run build-main && npm run build-renderer",
    "start": "cross-env NODE_ENV=production electron ./",
    "start-hot": "cross-env HOT=1 NODE_ENV=development electron -r babel-register -r babel-polyfill ./main.development",
    "postinstall": "node node_modules/fbjs-scripts/node/check-dev-engines.js package.json",
    "dev": "concurrently --kill-others \"npm run hot-server\" \"npm run start-hot\"",
    "cleanup": "mop -v",
    "package": "cross-env NODE_ENV=production node -r babel-register -r babel-polyfill package.js --asar=true",
    "package:all": "npm run package -- --all",
    "package:win": "npm run package -- --win",
    "installer:win": "electron-builder \"release/win32-x64/platmatic-win32-x64\" --platform=win --out=\"release\" --config=builder.json",
    "installer:mac": "electron-installer-dmg \"release/darwin-x64/platmatic-darwin-x64\" platmatic"
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "author": {
    "name": "David Ahern",
    "email": "david.ahern26@gmail.com"
  },
  "license": "MIT",
  "keywords": [
    "electron",
    "boilerplate",
    "react",
    "react-router",
    "flux",
    "webpack",
    "react-hot"
  ],
